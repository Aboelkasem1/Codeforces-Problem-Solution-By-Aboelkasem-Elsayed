void solve()
{
    cin >> n >> k;

    vi a(n); in(a);
    int count = 0;

    sort(a.rbegin(), a.rend());

    FORALL(i, a)
    {
        if (i >= a[k-1] && i != 0)
        {
            count++;
        }
    }


    cout << count << "\n";
}


